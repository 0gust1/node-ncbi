{
  "env": {
    "node": true,
    "commonjs": true
  },
  "globals": {

  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true
  },
  "rules": {
    "no-console": 0,
    "quotes": [2, "single"],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "prefer-const": [2]
  }
}
